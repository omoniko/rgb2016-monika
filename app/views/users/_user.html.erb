<tr>
      <th scope="row"><%= user.id %></th>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.age %></td>
      <td>
            <%= link_to "Edit", edit_user_path(user.id), class: 'btn btn-success' %>
            <%= link_to "Delete", user_path(user.id), method: :delete, class: 'btn btn-success' %>
            <%= link_to "Cars", user_cars_path(user.id), class: 'btn btn-success' %>
      </td>
      
    </tr>